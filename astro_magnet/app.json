{
  "expo": {
    "name": "astromagnet",
    "slug": "astromagnet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/background.png",
      "resizeMode": "contain",
      "backgroundColor": "#9A48D0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/b06b77b1-184c-4021-b318-8ab5833305e6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.astro.magnet.pro",
      "googleServicesFile": "./firebaseConfig/GoogleService-Info.plist",
      "plugins": [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    },
    "android": {
      "googleServicesFile": "./firebaseConfig/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.astro.magnet.pro"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-google-signin/google-signin"
    ],
    "extra": {
      "eas": {
        "projectId": "b06b77b1-184c-4021-b318-8ab5833305e6"
      }
    },
    "runtimeVersion": "1.0.0"
  }
}
